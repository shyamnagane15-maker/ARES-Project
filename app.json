{
  "expo": {
    "name": "ARES",
    "slug": "ares-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",  
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png", 
      "resizeMode": "contain",
      "backgroundColor": "#FFC0CB"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png", 
        "backgroundColor": "#FFC0CB"
      },
      "package": "com.ares.protection" 
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-notifications" 
    ]
  }
}
